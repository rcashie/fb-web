UPSERT INTO `published` (KEY k, VALUE v)
SELECT RAW {
    "k": "tset::char::" || $id,
    "v": {
        "game": char.game,
        "type": "tagset",
        "tags": ARRAY_CONCAT([char.title], char.tags),
        "parentTags": gameTagSet.tags
    }
}
FROM `published` AS char
USE KEYS["char::" || $id]
JOIN `published` AS gameTagSet ON KEYS["tset::game::" || char.game]
